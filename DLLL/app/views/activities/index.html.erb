 <h1>Activities</h1>
  <%= link_to 'Logout', session_path('dummy'), method: :delete %>
 <ul>
     <% @activities.each do |activity| %>
         <li>
             <strong> <%= activity.name %> </strong> - <%= activity.description %>
             <%= link_to "More info", activity_path(activity) %>
         </li>
     <% end %>
 </ul>
